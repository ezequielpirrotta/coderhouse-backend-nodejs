<div>
    <h1>Hola! {{user.name}}, que gusto verte!</h1>
    {{#if isAdmin}}
       {{#each food}}
          <div>
            <p>{{this.name}}</p>
            <p>{{this.price}}</p>
          </div>
       {{/each}}
